            }
        );

        /* try to delay guuru chat until the regular chat is ready, if this fails for some reason, guuru
            will get visible by timeout */
        chatItem = document.querySelector('.js-chatListItem');
        if (chatItem) {
            const showGuuru = () => {
                const guuruItem = document.querySelector('.js-guuruListItem');
                if (guuruItem) {
                    guuruItem.removeAttribute('hidden')
                }
            }

            const observeChatReadiness = (mutationList, observer) => {
                mutationList.filter(mutation => mutation.addedNodes && mutation.addedNodes[0].classList.contains('embeddedServiceHelpButton')).forEach(mutations => {
                    showGuuru();
                    chatReadinessObserver.disconnect();
                });
            };

            const chatReadinessObserver = new MutationObserver(observeChatReadiness);
            chatReadinessObserver.observe(chatItem, { childList: true });
        }

    };
