        //embedded_svc.settings.offlineSupportMinimizedText = '...'; //(Defaults to Contact Us)

        embedded_svc.settings.enabledFeatures = ['LiveAgent'];
        embedded_svc.settings.entryFeature = 'LiveAgent';

        embedded_svc.settings.prepopulatedPrechatFields = { FirstName: '',  LastName: '',  Email: '' };

        embedded_svc.settings.targetElement = document.querySelector('.embeddedService');

        embedded_svc.settings.extraPrechatInfo = [{
            'entityFieldMaps': [{
                    'doCreate':false,
                    'doFind':false,
                    'fieldName':'LastName',
